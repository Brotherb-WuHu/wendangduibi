
.content {
		display: flex;
		flex-direction: column;
		align-items: center;
		justify-content: center;
}
.logo {
		height: 200rpx;
		width: 200rpx;
		margin-top: 200rpx;
		margin-left: auto;
		margin-right: auto;
		margin-bottom: 50rpx;
}
.text-area {
		display: flex;
		justify-content: center;
}
.title {
		font-size: 36rpx;
		color: #8f8f94;
}
.btn{
	margin: 1rem 2rem;
	background-color: #007AFF;
	border-radius: 10rpx;
}
.btnImg{
		margin: 1.5rem ;
		width: 150rpx;
		height: 5rem;
}
